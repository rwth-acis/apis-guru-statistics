      "EndpointName": { "maxLength": 63, "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*", "type": "string" },
      "Header": { "maxLength": 1024, "type": "string" },
      "InternalFailure": {},
      "InvokeEndpointInput": {
